{
    "AllInputsPiece": {
        "name": "AllInputsPiece",
        "dependency": {
            "dockerfile": null,
            "requirements_file": "requirements_0.txt"
        },
        "tags": [
            "All Inputs"
        ],
        "style": {
            "node_label": "All Inputs",
            "node_type": "default",
            "node_style": {
                "backgroundColor": "#ebebeb"
            },
            "useIcon": true,
            "icon_class_name": "mdi:alert",
            "iconStyle": {
                "cursor": "pointer"
            }
        },
        "description": "This is an AllInputs Piece",
        "container_resources": {
            "requests": {
                "cpu": 100,
                "memory": 128
            },
            "limits": {
                "cpu": 500,
                "memory": 512
            }
        },
        "input_schema": {
            "$defs": {
                "InputEnum": {
                    "enum": [
                        "option1",
                        "option2",
                        "option3"
                    ],
                    "title": "InputEnum",
                    "type": "string"
                },
                "InputObject": {
                    "properties": {
                        "prop1": {
                            "default": null,
                            "description": "Argument name.",
                            "title": "Prop1",
                            "type": "string"
                        },
                        "prop2": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "items": {},
                                    "type": "array"
                                },
                                {
                                    "type": "integer"
                                },
                                {
                                    "type": "number"
                                },
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "object"
                                },
                                {
                                    "format": "date",
                                    "type": "string"
                                },
                                {
                                    "format": "date-time",
                                    "type": "string"
                                },
                                {
                                    "format": "time",
                                    "type": "string"
                                }
                            ],
                            "default": null,
                            "description": "Argument value.",
                            "from_upstream": "always",
                            "title": "Prop2"
                        }
                    },
                    "title": "InputObject",
                    "type": "object"
                }
            },
            "properties": {
                "input_string": {
                    "default": "default value",
                    "description": "Input string to be logged.",
                    "title": "Input String",
                    "type": "string"
                },
                "input_code": {
                    "default": "print('Hello world!')",
                    "description": "Input code to be logged.",
                    "title": "Input Code",
                    "type": "string",
                    "widget": "codeeditor"
                },
                "input_date": {
                    "default": "2023-01-01",
                    "description": "Input date to be logged.",
                    "format": "date",
                    "title": "Input Date",
                    "type": "string"
                },
                "input_time": {
                    "default": "16:20:00",
                    "description": "Input time to be logged.",
                    "format": "time",
                    "title": "Input Time",
                    "type": "string"
                },
                "input_datetime": {
                    "default": "2023-01-01T16:20:00",
                    "description": "Input datetime to be logged.",
                    "format": "date-time",
                    "title": "Input Datetime",
                    "type": "string"
                },
                "input_integer": {
                    "default": 10,
                    "description": "Input integer to be logged.",
                    "title": "Input Integer",
                    "type": "integer"
                },
                "input_float": {
                    "default": 10.5,
                    "description": "Input float to be logged.",
                    "title": "Input Float",
                    "type": "number"
                },
                "input_float2": {
                    "default": 0.1,
                    "description": "Float with min and max",
                    "exclusiveMaximum": 1.0,
                    "exclusiveMinimum": 0.0,
                    "title": "Input Float2",
                    "type": "number"
                },
                "input_boolean": {
                    "default": false,
                    "description": "Input boolean to be logged.",
                    "title": "Input Boolean",
                    "type": "boolean"
                },
                "input_enum": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/InputEnum"
                        }
                    ],
                    "default": "option1",
                    "description": "Input enum to be logged."
                },
                "input_string_optional": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Input enum to be logged.",
                    "title": "Input String Optional"
                },
                "input_array_string": {
                    "default": [
                        "default_1",
                        "default_2",
                        "default_3"
                    ],
                    "description": "Input array to be logged.",
                    "items": {
                        "type": "string"
                    },
                    "title": "Input Array String",
                    "type": "array"
                },
                "input_array_float": {
                    "default": [
                        10.1,
                        10.2,
                        10.3
                    ],
                    "description": "Input array to be logged.",
                    "items": {
                        "type": "number"
                    },
                    "title": "Input Array Float",
                    "type": "array"
                },
                "input_array_float_2": {
                    "default": [
                        10.1,
                        10.2,
                        10.3
                    ],
                    "description": "Input array to be logged.",
                    "items": {
                        "exclusiveMinimum": 0.0,
                        "type": "number"
                    },
                    "title": "Input Array Float 2",
                    "type": "array"
                },
                "input_array_integer": {
                    "default": [
                        10,
                        10,
                        10
                    ],
                    "description": "Input array to be logged.",
                    "items": {
                        "type": "integer"
                    },
                    "title": "Input Array Integer",
                    "type": "array"
                },
                "input_array_object": {
                    "default": [
                        {
                            "prop1": "",
                            "prop2": 1
                        }
                    ],
                    "items": {
                        "$ref": "#/$defs/InputObject"
                    },
                    "title": "Input Array Object",
                    "type": "array"
                }
            },
            "title": "InputModel",
            "type": "object"
        },
        "output_schema": {
            "properties": {
                "message": {
                    "description": "Output message to log",
                    "title": "Message",
                    "type": "string"
                }
            },
            "required": [
                "message"
            ],
            "title": "OutputModel",
            "type": "object"
        },
        "secrets_schema": null,
        "source_url": "https://github.com/nathan-vm/all_inputs_piece/tree/main/pieces/AllInputsPiece"
    }
}